<!doctype html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

    <title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>

    <style>
            {{{css}}}
    </style>
    <style media="print">
        body {
            font-size: 12px;
        }
    </style>

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300' rel='stylesheet' type='text/css'>

</head>
<body>

<div id="resume">

    {{#resume.basics}}
        <section class="heading">
            <header>
                <h1>{{name}}</h1>
                <h2 class="vr"></h2>
                <h2 class="label">{{label}}</h2>
                <div class="contact">
                    {{#if email}}
                        <span class="detail">
					<a href="mailto:{{email}}">{{email}}</a>
				</span>
                    {{/if}}
                    {{#if phone}}
                        <span class="detail">
                            {{phone}}
                        </span>
                    {{/if}}
                    {{#if website}}
                        <span class="detail">
					<a href="{{website}}">{{website}}</a>
				</span>
                    {{/if}}
                </div>
            </header>


            {{#if summary}}
                <p>{{paragraphSplit summary}}</p>
            {{/if}}

            {{#if profiles.length}}
                {{#each profiles}}
                    <div class="profile">
                        {{#if network}}
                            <strong class="network">
                                {{network}}
                            </strong>
                        {{/if}}
                        {{#if username}}
                            <span class="username">
                                {{#if url}}
                                    <a href="{{url}}">{{username}}</a>
                                {{else}}
                                    {{username}}
                                {{/if}}
                            </span>
                        {{/if}}
                    </div>
                {{/each}}
            {{/if}}

        </section>
    {{/resume.basics}}

    {{#if resume.work.length}}
        <section class="experience">
            <h2>Experience</h2>
            {{#each_with_sort resume.work "priority" false}}
                <section class="item">
                    <header>
                        {{#if company}}
                            <h3 class="name">{{company}}</h3>
                        {{/if}}

                    </header>
                    {{#if position}}
                        <span class="position">
                            {{position}}
                        </span>
                    {{/if}}



                    {{#if startDate}}
                        <span class="startDate">
                            {{date startDate}}
                        </span>
                    {{/if}}
                    {{#if endDate}}
                        <span class="endDate">
				- {{date endDate}}
			</span>
                    {{else}}
                        <span class="endDate">
				- Present
			</span>
                    {{/if}}

                    {{#if summary}}
                        <div class="summary">
                            <p>{{paragraphSplit summary}}</p>
                        </div>
                    {{/if}}

                    {{#if highlights.length}}
                        <ul class="highlights">
                            {{#each highlights}}
                                <li>{{.}}</li>
                            {{/each}}
                        </ul>
                    {{/if}}

                    {{#if projects.length}}
                        <br/>
                        {{#if projectsLabel}}
                            <h4>{{projectsLabel}}</h4>
                            {{else}}
                            <h4>Projects at {{company}}</h4>

                        {{/if}}

                        <ul class="highlights">
                            {{#each_with_sort projects "startDate" true }}
                                <section class="item">
                                    <header>
                                        {{#if title}}
                                            <h3 class="project-name">{{title}}</h3>
                                        {{/if}}
                                    </header>
                                    {{#if position}}
                                        <span class="position">
                                            {{position}}
                                        </span>
                                    {{/if}}
                                    {{#if startDate}}
                                        <span class="startDate">
                                            {{dateYear startDate}}
                                        </span>
                                    {{/if}}
                                    {{#if noEndDate}}
                                    {{else}}
                                        {{#if endDate}}
                                        <span class="endDate">
                                        - {{date endDate}}
                                        </span>
                                        {{else}}
                                            <span class="endDate">
                                                - Present
                                            </span>
                                        {{/if}}
                                    {{/if}}

                                    {{#if summary}}
                                        <div class="summary">
                                            <p>{{paragraphSplit summary}}</p>
                                        </div>
                                    {{/if}}

                                    {{#if highlightsDISABLED.length}}
                                        <ul class="highlights">
                                            {{#each highlights}}
                                                <li>{{.}}</li>
                                            {{/each}}
                                        </ul>
                                    {{/if}}

                                </section>
                            {{/each_with_sort}}
                        </ul>
                    {{/if}}
                    {{#if clients.length}}
                        <br/>
                        <h4>Clients at {{company}}</h4>

                        <ul class="highlights">
                            {{#each clients}}
                                <section class="item">
                                    <header>
                                        {{#if title}}
                                            <h3 class="highlight-name">{{title}}</h3>
                                        {{/if}}
                                    </header>
                                    {{#if position}}
                                        <span class="position">
                                            {{position}}
                                        </span>
                                    {{/if}}
                                    {{#if startDate}}
                                        <span class="startDate">
                                            {{date startDate}}
                                        </span>
                                    {{/if}}
                                    {{#if endDate}}
                                        <span class="endDate">
                                        - {{date endDate}}
                                    </span>
                                                            {{else}}
                                                                <span class="endDate">
                                        - Present
                                    </span>
                                    {{/if}}

                                    {{#if summary}}
                                        <div class="summary">
                                            <p>{{paragraphSplit summary}}</p>
                                        </div>
                                    {{/if}}

                                    {{#if highlights.length}}
                                        <ul class="highlights">
                                            {{#each highlights}}
                                                <li>{{.}}</li>
                                            {{/each}}
                                        </ul>
                                    {{/if}}

                                </section>
                            {{/each}}
                        </ul>
                    {{/if}}
                </section>
            {{/each_with_sort}}
        </section>
    {{/if}}



    {{#if resume.clients.length}}
        <h2>Clients</h2>
        <ul class="highlights">
            {{#each resume.projects}}
                <li>{{title}}</li>
            {{/each}}
        </ul>
    {{/if}}

    {{#if resume.skills.length}}
        <section id="skills">
            <h2>Competencies</h2>
            {{#each resume.skills}}
                <div class="item">
                    {{#if name}}
                        <div class="name">
                            {{name}}
                        </div>
                    {{/if}}
                    {{#if keywords.length}}
                        <div class="keywords">
                            {{#each keywords}}
                                {{.}}{{#if @last}}{{else}},{{/if}}
                            {{/each}}
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.education.length}}
        <section id="education">
            <h2>Education</h2>
            {{#each resume.education}}
                <div class="item">
                    <header>
                        {{#if institution}}
                            <h3 class="name">{{institution}}</h3>
                        {{/if}}
                    </header>

                    <div class="dates">
                        {{#if startDate}}
                            <span class="startDate">
                                {{date startDate}}
                            </span>
                        {{/if}}
                        {{#if endDate}}
                            <span class="endDate">
					- {{date endDate}}
				</span>
                        {{else}}
                            <span class="endDate">
					- Present
				</span>
                        {{/if}}
                    </div>

                    <div class="qualification">
                        {{#if studyType}}
                            <span class="studyType">
                                {{studyType}}
                            </span>
                        {{/if}}

                        {{#if area}}
                            <span class="area">
					- {{area}}
				</span>
                        {{/if}}

                        {{#if gpa}}
                            <span class="gpa">
					GPA: {{gpa}}
				</span>
                        {{/if}}
                    </div>

                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.awards.length}}
        <section id="awards">
            <h2>Awards and Recognitions</h2>
            {{#each resume.awards}}
                <div class="item">
                    {{#if title}}
                        <div class="title">
                            {{title}}
                        </div>
                    {{/if}}
                    {{#if date}}
                        <div class="date">
                            <span class="position">{{awarder}}</span>
                            <span class="startDate">{{year date}}</span>
                        </div>
                    {{/if}}
                    {{#if summary}}
                        <div class="summary">
                            <p>{{summary}}</p>
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.publications.length}}
        <section id="publications">
            <h2>Publications</h2>
            {{#each resume.publications}}
                <div class="item">
                    {{#if name}}
                        <div class="name">
                            {{name}}
                        </div>
                    {{/if}}
                    {{#if publisher}}
                        <div class="publisher">
                            {{publisher}}
                        </div>
                    {{/if}}
                    {{#if releaseDate}}
                        <div class="releaseDate">
                            {{releaseDate}}
                        </div>
                    {{/if}}
                    {{#if summary}}
                        <div class="summary">
                            <p>
                                {{summary}}
                                {{#if website}}
                                    <span><a href="{{website}}">Play Store</a></span>
                                {{/if}}
                            </p>
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.languages.length}}
        <section id="languages">
            <h2>Languages</h2>
            {{#each resume.languages}}
                <div class="item">
                    {{#if language}}
                        <div class="language">
                            {{language}}
                        </div>
                    {{/if}}
                    {{#if fluency}}
                        <div class="fluency">
                            <em>{{fluency}}</em>
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.interests.length}}
        <section id="interests">
            <h2>Personal Interests</h2>
            {{#each resume.interests}}
                <div class="item">
                    {{#if name}}
                        <h5 class="name">
                            {{name}}
                        </h5>
                    {{/if}}
                    {{#if summary}}
                        <div class="summary">
                            {{summary}}
                        </div>
                    {{/if}}
                    {{#if keywords.length}}
                        <ul class="keywords">
                            {{#each keywords}}
                                <li>{{.}}</li>
                            {{/each}}
                        </ul>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

    {{#if resume.references.length}}
        <section id="references">
            <h2>References</h2>
            {{#each resume.references}}
                <div class="item">
                    {{#if reference}}
                        <blockquote class="reference">
                            {{reference}}
                        </blockquote>
                    {{/if}}
                    {{#if name}}
                        <div class="name">
                            — {{name}}
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </section>
    {{/if}}

</div>

</body>
</html>
